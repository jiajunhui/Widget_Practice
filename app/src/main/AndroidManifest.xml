<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kk.taurus.customviewtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".TestCircleProgressActivity"/>
        <activity android:name=".TestRadarViewActivity"/>
        <activity android:name=".TestBezierViewActivity"/>
        <activity android:name=".TestCircleArrowViewActivity"/>
        <activity android:name=".TestSlideIndexBarActivity"/>
        <activity android:name=".TestSearchIconViewActivity"/>
        <activity android:name=".TestFEViewActivity"/>
        <activity android:name=".TestChartViewActivity"/>
        <activity android:name=".TestNumberEditViewActivity"/>
        <activity android:name=".TestRangeProgressViewActivity"/>
        <activity
            android:screenOrientation="landscape"
            android:name=".TestMultiplicationViewActivity"/>
    </application>

</manifest>